{
  "name": "@holusion/theme",
  "version": "1.1.0",
  "private": false,
  "repository": "https://github.com/Holusion/holusion.com",
  "license": "MIT",
  "main": "dist/bootstrap.css",
  "scripts": {
    "test": "mocha --slow 1500 --timeout 10000 ./scripts/runner.js",
    "jenkins_test": "JUNIT_REPORT_PATH=report.xml mocha ./scripts/runner.js --reporter mocha-jenkins-reporter --timeout 15000",
    "prepack": "mkdir -p dist/ && cp _site/css/bootstrap.* dist/ && cp node_modules/bootstrap/dist/js/bootstrap.min.js* dist/ && sed 's/..\\/node_modules\\//~/' _css/_variables.scss > dist/_variables.scss",
    "postpack": "rm -rf dist/"
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "jquery": "1.9.1 - 3",
    "popper.js": "^1.14.7"
  },
  "devDependencies": {
    "bootstrap": "^4.5.2",
    "chai": "^4.2.0",
    "cookieconsent": "^3.0.6",
    "faker": "^4.1.0",
    "finalhandler": "^1.1.1",
    "firebase-tools": "^8.11.2",
    "jquery": "^3.5.1",
    "mocha": "^8.1.1",
    "mocha-jenkins-reporter": "0.4.5",
    "popper.js": "^1.14.7",
    "puppeteer": "^5.2.1",
    "serve-static": "^1.13.2",
    "xml-parser": "^1.2.1"
  }
}

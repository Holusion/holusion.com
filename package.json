    "postpack": "rm -rf dist/"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "faker": "^4.1.0",
    "finalhandler": "^1.1.1",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "0.3.9",
    "puppeteer": "^1.15.0",
    "serve-static": "^1.13.2",
    "xml-parser": "^1.2.1",
    "bootstrap": "^4.3.1",
    "cookieconsent": "^3.0.6",
    "jquery": "^3.4.0",
    "popper.js": "^1.14.7"
  }
}
